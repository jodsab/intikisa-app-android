{
  "expo": {
    "name": "Intikisa Per√∫",
    "slug": "intikisa-app-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logointikisa.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "intikisa.app",
      "permissions": ["CAMERA", "RECORD_AUDIO", "READ_CONTACTS", "READ_PHONE_STATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
